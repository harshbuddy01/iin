{
  "rewrites": [
    {
      "source": "/admin-dashboard-v2",
      "destination": "/admin-dashboard-v2.html"
    },
    {
      "source": "/admin-login",
      "destination": "/admin-login.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}